{
    "name": "nonz250/storage-php-client",
    "description": "This is nonz250's storage api client.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StoragePhpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nozomi Hosaka",
            "email": "hosaka.non.work@gmail.com"
        }
    ],
    "scripts": {
        "setup": [
            "composer install",
            "composer dump-autoload"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "php-http/mock-client": "^1.6",
        "psr/http-factory-implementation": "*",
        "guzzlehttp/guzzle": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "require": {
        "ext-mbstring": "*",
        "php": "^8.1",
        "fig/http-message-util": "^1.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
